{
  "name": "@./api-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "api-e2e",
    "implicitDependencies": [
      "github-dashboard-api"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "packages/github-dashboard/api-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "github-dashboard-api:build",
          "github-dashboard-api:serve"
        ]
      },
      "e2e:watch": {
        "executor": "@nx/jest:jest",
        "options": {
          "jestConfig": "packages/github-dashboard/api-e2e/jest.config.ts",
          "watch": true,
          "passWithNoTests": true
        }
      },
      "e2e:ci": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "packages/github-dashboard/api-e2e/jest.config.ts",
          "ci": true,
          "coverage": true,
          "passWithNoTests": true
        },
        "dependsOn": [
          "github-dashboard-api:build",
          "github-dashboard-api:serve"
        ]
      }
    }
  }
}